QPushButton {
    background-color: #007aff;            /* iOS blue */
    color: white;
    border: none;
    border-radius: 23px;                   /* pill-shaped */
    padding: 12px 33px;
    font-weight: 600;
    font-size: 17px;
    box-shadow: 0 4px 19px rgba(0, 122, 255, 0.3);
    transition: background-color 0.3s ease, box-shadow 0.3s ease;
}

QPushButton:hover {
    background-color: #0061cc;            /* slightly darker blue */
    box-shadow: 0 6px 12px rgba(0, 94, 203, 0.45);
}

QPushButton:pressed {
    background-color: #0050a0;
    box-shadow: inset 0 8px 16px rgba(0,0,0,0.45);

}
/* Modern iOS-style LineEdit with glass effect */
QLineEdit {
    background-color: rgba(255, 255, 255, 0.750);  /* شبه شفاف */
    backdrop-filter: blur(12px);                   /* تأثير الزجاج */
    border: 1.5px solid rgba(255, 255, 255, 0.4);  /* حد خفيف */
    border-radius: 23px;                           /* أكثر دائرية */
    padding: 14px 20px;
    font-size: 17px;
    color: #1c1c1e;
    font-family: -apple-system, &quot;SF Pro Text&quot;, &quot;Segoe UI&quot;, Roboto, Oxygen,
        Ubuntu, Cantarell, &quot;Helvetica Neue&quot;, Arial, sans-serif;
    box-shadow: inset 0 1px 2px rgba(0,0,0,0.08);
    transition: all 0.3s ease;
}

/* عند التركيز يظهر إطار أزرق داخلي */
QLineEdit:focus {
    border: 1.5px solid #007aff;
    box-shadow: 0 0 12px rgba(0, 122, 255, 0.25);
    outline: none;
}

/* Placeholder text */
QLineEdit::placeholder {
    color: rgba(28, 28, 30, 0.5);
    font-style: italic;
}
